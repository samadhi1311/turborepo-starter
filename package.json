{
	"name": "turborepo-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prebuild": "prettier --write \"**/*.{ts,tsx,md,json}\"",
		"build": "turbo build",
		"dev": "turbo dev",
		"lint": "turbo lint",
		"format": "prettier --write \"**/*.{ts,tsx,md,json}\""
	},
	"devDependencies": {
		"@workspace/eslint-config": "workspace:*",
		"@workspace/typescript-config": "workspace:*",
		"prettier": "^3.6.2",
		"turbo": "^2.5.8",
		"typescript": "5.9.2"
	},
	"packageManager": "pnpm@10.16.0",
	"engines": {
		"node": ">=22"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"core-js-pure"
		]
	}
}

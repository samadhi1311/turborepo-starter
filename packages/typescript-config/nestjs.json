{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Nest.js",
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"removeComments": true,
		"incremental": true,
		"skipLibCheck": true,
		"noEmit": false,
		"outDir": "./dist",
		"declaration": true,
		"sourceMap": true
	}
}
